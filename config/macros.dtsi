macro_filestructure: macro_filestructure{
compatible = "zmk,behavior-macro";
label = "macro_filestructure";
#binding-cells = <0>;
wait-ms = <5>;
bindings = <&macro_press &mo 2 &kp LGUI>, <&macro_tap &kp F12>, <&macro_release &mo 2 &kp LGUI>;
};
macro_iterm: macro_iterm{
compatible = "zmk,behavior-macro";
label = "macro_iterm";
#binding-cells = <0>;
wait-ms = <5>;
bindings = <&macro_press &kp LCTRL>, <&macro_tap &kp GRAVE>, <&macro_release &kp LCTRL>;
};
